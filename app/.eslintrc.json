{
  "parser": "babel-eslint",
  "extends": [
      "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["import"],
  "rules": {
    "indent": ["error", 4],
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react/prefer-stateless-function": "off"
  }
}
